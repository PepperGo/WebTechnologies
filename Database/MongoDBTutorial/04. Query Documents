db.employees.findOne()
//
{ 
    "_id" : ObjectId("59b3735a0e33db88dba4b6fd"), 
    "EmpNo" : "1", 
    "FirstName" : "Andrew", 
    "LastName" : "Neil", 
    "Age" : "30", 
    "Gender" : "Male", 
    "Skill" : "MongoDB", 
    "Phone" : "408-1234567", 
    "Email" : "Andrew.Neil@gmail.com", 
    "Salary" : "80000"
}

db.employees.find({
        "EmpNo" : "2"
})
//
{ 
    "_id" : ObjectId("59b373f40e33db88dba4b6fe"), 
    "EmpNo" : "2", 
    "FirstName" : "Brian", 
    "LastName" : "Hall", 
    "Age" : "27", 
    "Gender" : "Male", 
    "Skill" : "Javascript", 
    "Phone" : "408-1298367", 
    "Email" : "Brian.Hall@gmail.com", 
    "Salary" : "60000"
}


db.employees.find({
        "Age" : {$lt: "30"}
})
//
{ 
    "_id" : ObjectId("59b373f40e33db88dba4b6fe"), 
    "EmpNo" : "2", 
    "FirstName" : "Brian", 
    "LastName" : "Hall", 
    "Age" : "27", 
    "Gender" : "Male", 
    "Skill" : "Javascript", 
    "Phone" : "408-1298367", 
    "Email" : "Brian.Hall@gmail.com", 
    "Salary" : "60000"
}
